{
  "name": "admin",
  "version": "1.9.0",
  "private": true,
  "scripts": {
    "dev": "npm run local:serve",
    "preview": "vite preview",
    "serve": "npm run local:serve",
    "local:serve": "export VITE_FIREBASE_USE_EMULATORS=true; vite --mode develop",
    "develop:serve": "vite --mode develop",
    "staging:serve": "vite --mode staging",
    "production:serve": "vite --mode production",
    "develop:build": "vite build --mode develop;",
    "staging:build": "vite build --mode staging",
    "production:build": "vite build --mode production",
    "develop:deploy": "vite build --mode develop; firebase deploy --only hosting:develop-admin-app -P develop",
    "staging:deploy": "vite build --mode staging; firebase deploy --only hosting:staging-admin-app -P staging",
    "production:deploy": "vite build --mode production; firebase deploy --only hosting:production-admin-app -P production",
    "develop:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:develop-admin-app -P develop",
    "staging:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:staging-admin-app -P staging",
    "production:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:production-admin-app -P production",
    "test": "vite test:unit",
    "test:unit": "vite test:unit",
    "test-ci": "vite test:unit --ci --runInBand --silent --forceExit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint-ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --no-fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@jac-uk/jac-kit": "3.0.40",
    "@ministryofjustice/frontend": "0.2.4",
    "@rollup/plugin-inject": "^5.0.3",
    "@sentry/tracing": "^7.61.1",
    "@sentry/vue": "^7.61.1",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compat": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "buffer": "^6.0.3",
    "chart.js": "^4.2.1",
    "detect-browser": "^5.3.0",
    "firebaseui": "^4.8.0",
    "govuk-frontend": "^3.12.0",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "patternomaly": "^1.3.2",
    "save-file": "^2.3.1",
    "stream-browserify": "^3.0.0",
    "vue": "^3.1.0",
    "vue-chartjs": "^5.2.0",
    "vue-dompurify-html": "^3.1.2",
    "vue-json-pretty": "^2.2.3",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.1.0",
    "bufferutil": "^4.0.3",
    "clone": "^2.1.2",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^8.7.1",
    "firebase": "^8.10.1",
    "firebase-mock": "^2.3.2",
    "postcss": "8.2.15",
    "process": "^0.11.10",
    "sass": "^1.32.13",
    "utf-8-validate": "^5.0.4",
    "vite": "^2.5.4",
    "vite-plugin-eslint": "^1.8.1"
  },
  "engines": {
    "node": "v16.15.1",
    "npm": "^8.11.0"
  },
  "peerDependencies": {
    "postcss": "8.2.15"
  }
}

{
  "name": "admin",
  "version": "0.105.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode develop",
    "staging:serve": "vue-cli-service serve --mode staging",
    "production:serve": "vue-cli-service serve --mode production",
    "develop:build": "vue-cli-service build --mode develop;",
    "staging:build": "vue-cli-service build --mode staging",
    "production:build": "vue-cli-service build --mode production",
    "develop:deploy": "vue-cli-service build --mode develop; firebase deploy --only hosting:develop-admin-app -P develop",
    "staging:deploy": "vue-cli-service build --mode staging; firebase deploy --only hosting:staging-admin-app -P staging",
    "production:deploy": "vue-cli-service build --mode production; firebase deploy --only hosting:production-admin-app -P production",
    "develop:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:develop-admin-app -P develop",
    "staging:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:staging-admin-app -P staging",
    "production:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:production-admin-app -P production",
    "test": "vue-cli-service test:unit",
    "test:unit": "vue-cli-service test:unit",
    "test-ci": "vue-cli-service test:unit --ci --runInBand --silent --forceExit",
    "lint": "vue-cli-service lint",
    "lint-ci": "vue-cli-service lint --no-fix"
  },
